{
  "version": "1",
  "rules": [
    {
      "trigger": "CUSTOM CMD",
      "description": "Password masking rule for As400Module",
      "search": "(\\sPASSWORD\\s*\\(\\s*|\\s*aName\\[userpassword\\]\\s+aValue\\[\\s*)(.*?)(\\s*\\)|\\s*\\])",
      "replace": "$1******$3"
    },
    {
      "trigger": "Replaced RDN",
      "description": "Password masking rule for As400Module",
      "search": "(\\sPASSWORD\\s*\\(\\s*|\\s*aName\\[userpassword\\]\\s+aValue\\[\\s*)(.*?)(\\s*\\)|\\s*\\])",
      "replace": "$1******$3"
    },
    {
      "trigger": "Attrs to replace within Configured Command",
      "description": "Password masking rule for As400Module",
      "search": "(\\sPASSWORD\\s*\\(\\s*|\\s*aName\\[userpassword\\]\\s+aValue\\[\\s*)(.*?)(\\s*\\)|\\s*\\])",
      "replace": "$1******$3"
    }
  ]
} 
